// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Habit {
  id String @id @default(uuid())

  title String

  created_ar DateTime

  @@map("habits")
}
